{
	"name": "my-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"deploy": "npm run build && npm run copy && npm run build_remote",
		"build_remote": "gh-pages -d build -t true",
		"copy": "mkdir -p build/zvezoskop && cp -rf build/smui.css build/zvezoskop/smui.css && cp -rf build/_app/ build/zvezoskop/_app/ && cp -rf static/index.html build/index.html",
		"update_data": "node data/update_data.js"
	},
	"devDependencies": {
		"@smui-extra/accordion": "^7.0.0-beta.15",
		"@smui-extra/autocomplete": "^7.0.0-beta.15",
		"@smui/button": "^7.0.0-beta.15",
		"@smui/dialog": "^7.0.0-beta.16",
		"@smui/icon-button": "^7.0.0-beta.15",
		"@smui/list": "^7.0.0-beta.15",
		"@smui/menu": "^7.0.0-beta.15",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.27.4",
		"d3-drag": "^3.0.0",
		"d3-force": "^3.0.0",
		"d3-scale": "^4.0.2",
		"d3-selection": "^3.0.0",
		"gh-pages": "^6.0.0",
		"googleapis": "^128.0.0",
		"layercake": "^8.0.2",
		"moment": "^2.30.1",
		"sass": "^1.69.5",
		"smui-theme": "^7.0.0-beta.15",
		"svelte": "^4.0.5",
		"svelte-body": "^1.4.0",
		"svelte-portal": "^2.2.0",
		"svelte-typewriter": "^3.2.2",
		"vite": "^4.4.2",
		"vite-imagetools": "^6.2.4"
	},
	"type": "module",
	"dependencies": {
		"material-icons": "^1.13.12",
		"moment": "^2.30.1",
		"svelte-waypoint": "^0.1.4",
		"sveltekit-i18n": "^2.3.2"
	}
}
